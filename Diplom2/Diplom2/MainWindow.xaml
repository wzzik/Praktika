        mc:Ignorable="d"
        Title="АРМ инженера службы логистики" Height="550" Width="1050" >
    <Grid>
        <!--<Frame Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden">
        </Frame>-->
        <StackPanel Orientation="Horizontal"  HorizontalAlignment="Stretch" >
